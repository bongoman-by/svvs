{
  "npmScope": "svvs",
  "affected": {
    "defaultBase": "HEAD"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },

  "projects": {
    "admin": {
      "tags": ["scope: admin", "type: app"]
    },
    "admin-e2e": {
      "tags": [],
      "implicitDependencies": ["admin"]
    },
    "api": {
      "tags": ["scope: api", "type: app"]
    },
    "api-interfaces": {
      "tags": []
    },
    "nx-angular-next": {
      "tags": ["scope: nx-angular-next", "type: app"]
    },
    "nx-angular-next-e2e": {
      "tags": ["scope: nx-angular-next-e2e"],
      "implicitDependencies": ["nx-angular-next"]
    },
    "shared-data-access-interfaces": {
      "tags": ["scope: shared", "type: lib", "lib-type:data-access"]
    },
    "ui-component": {
      "tags": ["scope: ui-lib", "type: lib"]
    }
  }
}
